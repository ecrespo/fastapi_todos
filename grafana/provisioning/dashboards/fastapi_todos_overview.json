{
  "id": null,
  "uid": "fastapi-todos-overview",
  "title": "FastAPI Todos - Overview",
  "tags": ["fastapi", "todos", "prometheus"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "row",
      "title": "HTTP",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "HTTP requests rate (by status)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "sum by (status) (rate(http_requests_total[5m]))",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "req/s" },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "HTTP requests rate (by method)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 },
      "targets": [
        {
          "expr": "sum by (method) (rate(http_requests_total[5m]))",
          "legendFormat": "{{method}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "req/s" },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "p95 HTTP latency (seconds)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 9 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Average HTTP latency (seconds)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 9 },
      "targets": [
        {
          "expr": "sum(rate(http_request_duration_seconds_sum[5m])) / sum(rate(http_request_duration_seconds_count[5m]))",
          "legendFormat": "avg",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      }
    },
    {
      "type": "row",
      "title": "Database",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 17 }
    },
    {
      "type": "timeseries",
      "title": "DB connection attempts",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 18 },
      "targets": [
        {
          "expr": "sum by (result) (rate(db_connection_attempts_total[5m]))",
          "legendFormat": "{{result}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "ops/s" },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "DB queries (rate by result)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 18 },
      "targets": [
        {
          "expr": "sum by (result) (rate(db_queries_total[5m]))",
          "legendFormat": "{{result}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "ops/s" },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "DB p95 query duration (seconds)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 26 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(db_query_duration_seconds_bucket[5m])))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "DB sessions in use",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 26 },
      "targets": [
        {
          "expr": "avg(db_sessions_in_use)",
          "legendFormat": "sessions",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "none" },
        "overrides": []
      }
    }
  ],
  "time": { "from": "now-1h", "to": "now" },
  "templating": { "list": [] }
}
